import "testing_buffering_station_svg.cif";

alg bool s_Tstartbutton = startBttn.state;
event e_startBttnClicked;

startBttn: Button(e_startBttnClicked);
button_svg: Button_svg("Start", e_startBttnClicked, s_Tstartbutton);

automaton def Button(event Clicked):
	alg bool state;
	location Released:
		initial;
		equation state = false;
		edge Clicked goto Pressed;
	location Pressed:
		equation state = true;
		edge Clicked goto Released;
end


